  "repository": {
    "type": "git",
    "url": "https://github.com:AlaeddineMessadi/hp-coding-challenge.git"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "swiper": "^4.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^6.2.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.11.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "moment": "^2.22.2",
    "node-sass": "^4.9.3",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.5.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
